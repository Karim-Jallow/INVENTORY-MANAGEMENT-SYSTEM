    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>aspnet-INVENTORY_MANAGEMENT_SYSTEM-be2d3df1-606d-4187-9ea7-e0b73c3d2247</UserSecretsId>
